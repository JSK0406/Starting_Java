//2
		Scanner sc = new Scanner(System.in);
		
		System.out.println("소수 구하기 프로그램");
		System.out.print("소수를 구할 정수 한개 입력:");
		int num = sc.nextInt();
		
		for (int i = 2; i <= num; i++) {
			for (int div = 2; div < i; div++) {
				if (i % div == 0) {
					break;
				}
			System.out.printf("%d은(는) 소수이다.\n", i);
			break
			}
		}

		
		
