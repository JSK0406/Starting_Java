package edu.forexone;

public class ForExOne0127 {

	public static void main(String[] args) {
		int sum = 0;
		for (int i = 1; i <= 10; i++) {
			sum += i;
		}
		System.out.println(sum);
		System.out.println(func(10));
	}

	public static int func(int final) {
		int res = 0;
		for (int i = 1; i <= final; i++) {
			res += i;
		}
		return res;
	}
}
