package edu.switchtwo;

public class SwitchSecond {

	public static void main(String[] args) {
		System.out.println(WhoIsIt("호랑이"));
		WhoIsIt("사자");
		WhoIsIt("독수리");
		WhoIsIt("참새");
		WhoIsIt("고등어");
		WhoIsIt("참새");
		WhoIsIt("곰팡이");
	}
	
