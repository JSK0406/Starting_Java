	static String WhoIsIt(String bio) {
		switch(bio) {
		case "호랑이":
		case "사자":
			String kind = "포유류";
			break;
		case "독수리":
		case "참새":
			String kind = "조류";
			break;
		case "고등어":
			String kind = "어류";
			break;
		default:
			System.out.println("어이쿠");
			String kind = "...";
		}
		return kind;
	}
}
